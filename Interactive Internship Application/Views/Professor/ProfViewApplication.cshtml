@{
}

<!DOCTYPE html>
<meta charset="UTF-8">
<html>
<head><h2>Professor</h2>
    <title>
       Professor View Application
    </title>
</head>
<body>
    
    @{int count = 0;}
   <table width="80%" border="1" cellpadding="3">
       <tr>
           @Html.AntiForgeryToken()
           @foreach (var item in ViewBag.bigDict)
           {
               count++;
               <td>
               @item
               </td>

               if (count == 2)
               {
                   count = 0; //reset count
                   <tr></tr>
               }
           }
    </table>     
</body>
</html>